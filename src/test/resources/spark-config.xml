<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>dark</theme>

        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>https</protocol>

        <!-- title of the document -->
        <documentTitle>Adactin Hotel CucumberBDD Framework</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>Automated Tests</reportName>

        <!-- timestamp format -->
        <timeStampFormat>MMM dd, yyyy HH:mm:ss</timeStampFormat>

        <!-- custom javascript -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {

                });
            ]]>
        </scripts>

        <!-- custom styles -->
        <styles>
            <![CDATA[

            ]]>
        </styles>

        <!-- system information -->
        <systemInfo>
            <property key="os" value="Windows" />
            <property key="Tester" value="Tshifhiwa Sinugo" />
            <property key="project" value="AdactinHotelCucumberBDDFramework" />
            <property key="browser" value="Google Chrome" />
            <!-- add more properties as needed -->
        </systemInfo>

    </configuration>
</extentreports>
